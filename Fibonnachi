import java.util.Scanner;

public class Fibonnachi {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);
        
        System.out.print("Enter the number of terms: ");
        int n = scanner.nextInt();
        scanner.close();
        
        long[] fibonacci = generateFibonacciSequence(n);
        printFibonacciSequence(fibonacci);
    }
    
    public static long[] generateFibonacciSequence(int n) {
        long[] sequence = new long[n];
        sequence[0] = 0;
        if (n > 1) {
            sequence[1] = 1;
            for (int i = 2; i < n; i++) {
                sequence[i] = sequence[i - 1] + sequence[i - 2];
            }
        }
        return sequence;
    }
    
    public static void printFibonacciSequence(long[] sequence) {
        System.out.println("Fibonacci Sequence:");
        for (long num : sequence) {
            System.out.print(num + " ");
        }
    }
}
